  ::-webkit-scrollbar
    width 12px
    height 10px
    border 0
  ::-webkit-scrollbar-track
    background-color #f2f4f7
  ::-webkit-scrollbar-thumb
    background-color #fff
    border-radius 2px
    border: 1px solid #cecece 
  ::-webkit-scrollbar-corner
    background-color transparent
