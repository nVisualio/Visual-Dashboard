  // 颜色选择器样式
  .el-color-dropdown
    .el-color-dropdown__link-btn // 去掉重置颜色按钮
      display none

    .el-color-dropdown__value
      .el-input
        padding 2px 0
        height 28px
        float none

        .el-input__inner
          height 24px !important
          padding 0
          margin 0
          border-radius 0
          background-color #FFFFFF
          color #000000
          border 1px solid #AAAAAA
          padding-left 5px

  // 字段列表选择颜色框
  .fieldFontSetting
    height 186px
    width 149px
    background-color: #FFFFFF
    border 1px solid #888888
    position fixed
    z-index 2
    top 0
    font-size 12px
    .item
      height 24px
      line-height 24px
      margin 10px 0
      span
        vertical-align top
        padding-right 10px
      .el-color-picker
        height 24px
        width 60px
        .el-color-picker__trigger
          width inherit
          height inherit
          padding 0
          .el-color-picker__icon
            display none
      .el-input
        width 60px
        height 24px
        padding-right 0
        float none
        .el-input__inner
          border-radius 5px
          height inherit
          background-color #FFFFFF
          border 1px solid #999999
          color #222222
          padding 0
          text-align center
    .el-checkbox__label
      padding 5px
      font-size 12px
    .reset
      box-sizing border-box
      display inline-block
      border 1px solid #999999
      font-size 14px
      line-height 24px
      width 60px
      border-radius 5px
      background-color #69aeff
      color #FFFFFF
      cursor pointer
  // 列表样式指定
  // filter
  .filterButton
    cursor pointer
  .inputBox
    width 80%
    height 25px
    .el-input__inner
      height 25px
      line-height 25px
      padding 0 5px
      text-align center
      &:focus
        border-color #666666
  .threeDSize
    width 28% !important
    padding-right 1%
  .size
    width 40% !important
    padding-right 3%
  .lock,
  .unlock
    text-align left
    margin-left 2%
    width 8%
    cursor pointer
    transition all 0.3s
    font-size 16px
    font-weight 700
  .lock:hover
    color LimeGreen
  .unlock:hover
    color red
  // 选择器
  .selector
    width 90%
    background-color transparent
    padding 0
    margin 0

    .el-input,
    .is-focus
      padding 0
      height 28px
      float left
      width 100%
      background-color transparent

      .el-input__inner
        height 28px
        padding 0
        margin 0
        //border-radius 0
        background-color transparent
        color #000000
        border 1px solid transparent
        transition border 1s
        padding-left 5px
        &:hover
          border 1px solid #AAAAAA

      .el-input__suffix
        .el-select__caret
          line-height 14px

  // str
  .str,
  .field,
  .objectTrigger,
  .linkTrigger,
  .example,
  .limit
    display inline-block
    width 100%
    text-align center
    font-weight 700

  .field
    color #E35112
    cursor pointer

  .objectTrigger,
  .linkTrigger
    color #019000
    cursor pointer

  .example
    color #C203FF
    cursor pointer

  .limit
    color #C203FF
  .operation
    color #A25D0D
    cursor pointer
  .addField
    background-color #FF0000
    width 16px
    margin-left 5px
    padding 0 4px
    border-radius 3px
    color #FFFFFF
    cursor pointer
  // color
  .color
    width 100%
    height 100%

    .el-color-picker__trigger
      margin-top 2px
      display inline-block
      text-align center
      width 16px
      line-height 100%
      height 16px
      padding 0
      border 0

      .el-color-picker__color
        border 0

        .el-color-picker__color-inner
          border-radius 50%

      .el-color-picker__icon
        display none
  // pic
  .thumbnail
    height 14px
    max-width 100%
    cursor pointer

    .el-input__inner
      height 24px
      padding 0
      margin 0
      border-radius 0
      background-color #FFFFFF
      color #000000
      border 1px solid #AAAAAA
      padding-left 5px

  // fontSetting
  .fontSetting
    line-height 27px
    border 1px solid red
    padding 2px 35px 3px 35px
    cursor pointer
  // producePreview
  .producePreview
    display inline-block
    height 0
    width 65px
    //border-top 5px solid red

  //switch
  .switch
    .el-switch__core
      height 18px
      &::after
        top 0
//slider
.order
  .slider
    display inline-block
    vertical-align middle
    width 300px
    padding-right 15px
.addObject,
.addFieldDialog,
.addObjectTriggerDialog,
.addObjectAddClassDialog,
.addObjectAddChildrenClassDialog,
.addObjectAddBrandDialog
  .el-dialog
    background-color transparent
  .el-form-item__error
    position absolute
    top 8px
  .addObjectAddClass
    width 15px
    height 15px
    background-color #FF0000
    position absolute
    left 338px
    top 8px
    border-radius 5px
    font-size 15px
    line-height 15px
    color #FFFFFF
    cursor pointer
  .el-dialog__header
    height 35px
    padding 0
    background-color rgb(70, 68, 68)
    span
      color #FFFFFF
    .el-dialog__title
      float left
      padding-left 15px
      font-size 15px
      line-height 35px
      text-align left
    .el-dialog__headerbtn
      top 10px
      right 10px
      i
        color #FFFFFF

  .el-dialog__body
    padding 27px 40px
    background-color rgba(87, 87, 87, 0.898039215686275)

    .checkbox
      height 50px !important

      .el-form-item__content
        height inherit !important

        .el-checkbox.is-checked,
        .el-checkbox
          margin 0 30px
          height inherit
          line-height 50px
          //color #FFFFFF

          .el-checkbox__inner
            border 1px solid red

          .el-checkbox__label
            color #FFFFFF

        .el-checkbox.is-checked
          .el-checkbox__inner
            background-color #FFFFFF

            &:after
              border-color red

    .el-form-item
      margin 0

      .el-form-item__content
        line-height 32px
        .el-select
          width 177px
      .el-form-item__label
        line-height 32px
        color #FFFFFF
        position relative
        left 80px
        &::before
          position absolute
          left -8px

    .el-select
      position absolute
      left 150px

      .el-input
        left 0

    .el-input
      position absolute
      left 150px
      width 177px
      padding 0
      height 28px

      .el-input__inner
        border-radius 0
        background-color #424242
        color #FFFFFF
        height 28px
    .el-input__suffix
      .el-select__caret
        line-height 14px

    .el-button
      height 30px
      width 88px
      margin 0 15px
      padding 0
      line-height 30px
      border 0
      color #FFFFFF

      &:first-child
        background-color #919191

      &:last-child
        background-color #393939

    .divider
      border-bottom 1px dotted #AAAAAA
      margin 25px 0

.addObjectAddClassDialog,
.addObjectAddChildrenClassDialog,
.addObjectAddBrandDialog
  top 10vh
  .el-form-item__content
    .el-button
      position relative
      left 105px
.addFieldDialog,
.addObjectTriggerDialog
  .el-dialog__body
    .el-form
      .setColor
        .filterColor,
        .filterBackColor
          height 30px
          background-color #444444
          border-radius 5px
          .el-color-picker__trigger
            width 60px
            height 30px
            padding 0
            border 0
            .el-color-picker__color
              border 0
              .el-color-picker__color-inner
                border-radius 5px
#file-upload-button
  display none
