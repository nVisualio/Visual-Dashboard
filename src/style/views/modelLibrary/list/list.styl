$tabsColor = #000000
$tabIconColor = #FFFFFF
$tableToolBar = #555555
#modelLibraryMain
  flex-grow 1
  overflow auto
  display flex
  >>>.el-loading-spinner
    // background-image url('/loading.svg');
    background-repeat  no-repeat;
    background-size  200px 60px;
    height 100px;
    width 100%;
    background-position center;
    top 40%
    svg
      display none
  .tabs
    box-sizing border-box
    height 100%
    width 46px
    padding 16px 0
    background-color $tabsColor

    .tab
      //height 60px
      width: inherit
      color $tabIconColor
      cursor pointer
      opacity 0.5
      padding 5px 0

      &:hover
        opacity 1
      span
        font-size 12px
        line-height 16px
        display block
        zoom 0.8
      .icon
        line-height 18px
        padding 5px 0
        zoom 1
        font-size 20px
    .isClick
      opacity 1

  .table
    overflow auto
    height 100%
    max-height 100%
    flex 1 0 0
    .tableBody
      position relative
      height inherit
      display flex
      flex-direction column
      .header
        text-align left
        .bar
          display inline-block
          width 50%
          text-align right
          .tool
            display inline-block
            vertical-align middle
            line-height 40px
            overflow hidden
            height 40px
            transition all 1s
            .icon
              display inline-block
              width 40px
              line-height inherit
              font-size 18px
              text-align center
              cursor pointer
              &:hover
                background-color #EBEEF5
          .search
            display inline-block
            box-sizing border-box
            padding 5px 20px
            .el-input
              .el-input__inner
                height 30px
                line-height 30px
                width 220px
                &:focus
                  border 1px solid #DCDFE6
              .el-input__icon
                line-height 30px
        .el-breadcrumb
          display inline-block
          vertical-align middle
          height 40px
          padding 5px 0 5px 20px
          width 50%
          box-sizing border-box
          .el-breadcrumb__item
            line-height 30px
            .el-breadcrumb__inner
              display inline-block
              height inherit
              font-size 14px
              font-weight 600
              transition all 0.5s
              cursor pointer
              font-family Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif
              border-bottom 2px solid transparent
              &:hover
                color #409eff
                border-color skyblue
              .el-tooltip
                display inline-block
                padding 0 20px
      .tableComponent
        //height inherit
        display flex
        flex-direction column
        flex-grow 1
        .el-table
          overflow scroll
          display flex
          flex 1 0 0
          flex-direction column
          border 0
          &::before,
          &::after
            display none
          .el-table__body-wrapper
            flex-grow 1

    .el-table::before
      height 0
    .caret-wrapper
      position absolute
      top -5px
    .cell
      padding 0
      max-height 28px
      .inputText
        display inline-block
        width 80%
        height 25px
        line-height 25px
        cursor pointer
        border 1px solid #DCDFE6
        border-radius 5px
        box-sizing border-box
        transition border-color 0.5s
        &:hover
          border-color #999999
      .inputTextTwo
        width 38%!important
        margin 0 1%
      .inputTextThree
        width 26%!important
        margin 0 1%
    .el-table__body-wrapper
      height calc(100% - 42px)
      border-bottom 10px
      border-right 10px
      overflow scroll
      td
        border-right-color transparent
      .el-table__row
        height 3px
    .el-table__header-wrapper
      border-bottom 2px solid #69aeff
      .el-table__header
        box-sizing content-box
        padding-right 10px
      th
        border-style dotted
        border-color transparent
        transition border-right-color 1s
        font-weight normal
        color #222222
      &:hover
        th
          border-right-color #AAAAAA

    .tableToolBar
      height 49px
      background-color $tableToolBar
      color #FFFFFF

      .tableTool
        float left
        width 50px
        height inherit
        cursor pointer

        .img
          display block
          height 30px
          line-height 30px
          font-size 20px

        span
          height 19px
          line-height 19px
          font-size 12px
          zoom 0.8

  .filter
    position absolute
    background-color #FFFFFF
    top 90px
    left 10px
    //height 414px
    width 230px
    z-index 3
    border 1px solid #AAAAAA
    border-radius 10px
    .header
      margin-top 5px
      height 30px
      text-align right
      div
        text-align center
        height 30px
        line-height 30px
        font-size 20px
        width 30px
        transition transform 0.5s
        cursor pointer
        &:hover
          transform rotate(-180deg)

    .main
      padding 10px 30px
      display flex
      flex-wrap wrap
      align-content flex-start
      .el-switch
        width 150px
        height 26px
        flex-grow 1

    //.close
