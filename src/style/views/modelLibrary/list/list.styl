  .el-color-dropdown__btns
    .el-color-dropdown__value
      .el-input
        .el-input__inner
          width 120px
          padding 2px 0
          //height 28px
          background-color transparent
          margin 0
          color #000000
          border 1px solid #AAAAAA
          padding-left 5px
          transition  border 1s
          text-align center
